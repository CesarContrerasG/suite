<?xml version="1.0" encoding="utf-8"?>
    <Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:oxml="http://www.ventanillaunica.gob.mx/cove/ws/oxml/">
        <Header>
            <Security xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" mustUnderstand="1">
                <UsernameToken>
                    <Username/>
                    <Password Type="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText"/>
                </UsernameToken>
            </Security>
        </Header>
        <Body>
            <solicitarRecibirCoveServicio>
                <comprobantes>
                    <tipoOperacion/>
                    <patenteAduanal/>
                    <fechaExpedicion/>
                    <observaciones/>
                    <rfcConsulta/>
                    <tipoFigura/>
                    <correoElectronico/>
                    <firmaElectronica>
                        <certificado/>
                        <cadenaOriginal/>
                        <firma/>
                    </firmaElectronica>
                    <numeroFacturaOriginal/>
                    <factura>
                        <certificadoOrigen/>
                        <numeroExportadorAutorizado/>
                        <subdivision/>
                    </factura>
                    <emisor>
                        <tipoIdentificador/>
                        <identificacion/>        
                        <nombre/>
                        <domicilio>
                            <calle/>
                            <numeroExterior/>
                            <numeroInterior/>
                            <colonia/>
                            <localidad/>
                            <municipio/>
                            <entidadFederativa/>
                            <pais/>
                            <codigoPostal/>
                        </domicilio>
                    </emisor>
                    <destinatario>
                        <tipoIdentificador/>
                        <identificacion/>
                        <nombre/>
                        <domicilio>
                            <calle/>
                            <numeroExterior/>
                            <numeroInterior/>
                            <colonia/>
                            <localidad/>
                            <municipio/>
                            <entidadFederativa/>
                            <pais/>
                            <codigoPostal/>
                        </domicilio>
                    </destinatario>                    
                </comprobantes>
            </solicitarRecibirCoveServicio>
        </Body>
    </Envelope>

